{
    "name": "nusje2000/feature-toggle-bundle",
    "description": "A bundle for adding and managing feature toggles in your project",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Nusje2000\\FeatureToggleBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nusje2000\\FeatureToggleBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "myclabs/php-enum": "^1.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "symfony/cache": "^6.2",
        "symfony/config": "^6.2",
        "symfony/console": "^6.2",
        "symfony/dependency-injection": "^6.2",
        "symfony/event-dispatcher": "^6.2",
        "symfony/filesystem": "^6.2",
        "symfony/http-client": "^6.2",
        "symfony/http-foundation": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/polyfill-php80": "^1.22"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.18",
        "psalm/plugin-symfony": "^5.0",
        "twig/twig": "^3.0",
        "vimeo/psalm": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Maarten Nusteling",
            "email": "maarten.nusteling@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
