{
    "name": "nusje2000/feature-toggle-bundle",
    "description": "A bundle for adding and managing feature toggles in your project",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Nusje2000\\FeatureToggleBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nusje2000\\FeatureToggleBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2",
        "myclabs/php-enum": "^1.0",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/polyfill-php80": "^1.22",
        "thecodingmachine/safe": "^1.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.86",
        "phpstan/phpstan-phpunit": "^0.12.18",
        "phpstan/phpstan-symfony": "^0.12.33",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.15.1",
        "twig/twig": "^3.0",
        "vimeo/psalm": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Maarten Nusteling",
            "email": "maarten.nusteling@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
